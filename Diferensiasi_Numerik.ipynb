      "source": [
        "# Diferensiasi Numerik ##\n",
        "\n",
        "import math\n",
        "\n",
        "class dif:\n",
        "  def f(x):\n",
        "    return x**2\n",
        "\n",
        "  def bedaMaju(f,h,x):\n",
        "    return (f(x+h) - f(x)) / h\n",
        "\n",
        "  def bedaMundur(f,h,x):\n",
        "    return (f(x) - f(x-h)) / h\n",
        "\n",
        "  def bedaTengah(f,h,x):\n",
        "    return (f(x+h) - f(x-h)) / 2*h\n",
        "\n",
        "x = 2\n",
        "h = 0.01\n"
      ]
